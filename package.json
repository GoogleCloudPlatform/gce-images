{
  "name": "gce-images",
  "version": "0.3.4",
  "description": "Get a list of globally available Google Compute Engine images",
  "main": "src/index.js",
  "files": [
    "src/"
  ],
  "scripts": {
    "cover": "nyc --reporter=lcov mocha test/*.js && nyc report",
    "docs": "jsdoc -c .jsdoc.js",
    "lint": "eslint src/ samples/ test/",
    "prettier": "prettier --write src/*.js src/*/*.js samples/*.js samples/*/*.js test/*.js test/*/*.js system-test/*.js system-test/*/*.js",
    "test": "mocha test/*.js",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "system-test": "mocha system-test/*.js --timeout 600000"
  },
  "keywords": [
    "gce",
    "google",
    "compute",
    "engine",
    "cloud",
    "images",
    "ubuntu",
    "linux",
    "redhat",
    "rhel",
    "opensuse",
    "suse",
    "debian",
    "coreos",
    "centos"
  ],
  "repository": "stephenplusplus/gce-images",
  "author": "Stephen Sawchuk <sawchuk@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "codecov": "^3.0.4",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-prettier": "^2.6.1",
    "ink-docstrap": "^1.3.2",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^2.2.5",
    "nyc": "^12.0.2",
    "power-assert": "^1.6.0",
    "prettier": "^1.14.0"
  },
  "dependencies": {
    "arrify": "^1.0.0",
    "async": "^1.5.2",
    "google-auto-auth": "^0.7.1",
    "got": "^4.1.1"
  }
}
